<?xml version="1.0" encoding="utf-8"?>
<root xmlns:android="http://schemas.android.com/apk/res/android">
  <init>
    <log text="DolbyIO plugin init"/>
  </init>

  <gradleProperties>
    <insert>
org.gradle.jvmargs=-Xmx1536M --add-exports=java.base/sun.nio.ch=ALL-UNNAMED --add-opens=java.base/java.lang=ALL-UNNAMED --add-opens=java.base/java.lang.reflect=ALL-UNNAMED --add-opens=java.base/java.io=ALL-UNNAMED --add-exports=jdk.unsupported/sun.misc=ALL-UNNAMED
ANDROID_BUILD_TOOLS_GRADLE_VERSION=com.android.tools.build:gradle:7.3.1
ANDROID_TOOLS_BUILD_GRADLE_VERSION=com.android.tools.build:gradle:7.3.1
    </insert>
  </gradleProperties>

	<buildGradleAdditions>
		<insert>
repositories {
  mavenLocal()
  google()
  mavenCentral()
}

dependencies {
	implementation('io.dolby:comms-sdk-android-cppsdk:2.5.3')
}
		</insert>
	</buildGradleAdditions>
</root>
